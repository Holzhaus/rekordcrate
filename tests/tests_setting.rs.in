#[test]
#[allow(non_snake_case)]
fn setting_{name}() {{
    println!("Parsing file: {filepath}");
    let data = include_bytes!("{filepath}").as_slice();
    let mut reader = Cursor::new(data);
    let setting_type = SettingType::from_filename("{filepath}").expect("failed to determine settings file type");
    let setting = Setting::read_args(&mut reader, (setting_type,)).expect("failed to parse setting file");
    println!("Setting file: {{:#?}}", setting);

    let mut new_data = Vec::with_capacity(data.len());
    let mut writer = Cursor::new(&mut new_data);
    setting.write(&mut writer).expect("failed to write setting file");
    assert_eq!(data, new_data);
}}
